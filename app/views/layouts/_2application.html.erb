					<b class="r9"></b>
					<b class="r8"></b>
					<b class="r7"></b>
					<b class="r6"></b>
					<b class="r5"></b>
					<b class="r4"></b>
					<b class="r3"></b>
					<b class="r2"></b>
					<b class="r1"></b>
					<div class="message"> 
						<table>
							<tr><th class="table_1">Мы предлагаем:</th><th>У нас есть:</th></tr>
							<tr><td class="table_1">1 базу данных MySQL(50MB)</td><td>PHP 5</td></tr>
							<tr><td class="table_1">Прикрепление своих доменов</td><td>MySQL</td></tr>
							<tr><td class="table_1">Предустановленные движки сайтов</td><td>Perl</td></tr>
							<tr><td class="table_1">1 базу данных MySQL(50MB)</td><td>Ruby</td></tr>
						</table>
					</div>
					<b class="r1"></b>
					<b class="r2"></b>
					<b class="r3"></b>
					<b class="r4"></b>
					<b class="r5"></b>
					<b class="r6"></b>
					<b class="r7"></b>
					<b class="r8"></b>
					<b class="r9"></b>
					<b class="r10"></b>
			</div>	
			<div><%= image_tag("logo.png", :class => "logo") %></div>
			<div class="autorisation1">
			<% if not logged_in?%>
			<h5>Авторизация:</h5>
				<div class="autorisation2">
				<%- form_tag(url_for(:controller=>'account',:action => "login" )) do -%>
					Login:<%= text_field_tag 'login','', :size=>'20', :class=>'text' %>
					<%= hidden_field 'layer','layer', :value=>params[:layer] %>
				    Пароль:
					<%= password_field_tag 'password','', :size=>'20', :class=>'text' %>
					<div><%= check_box_tag 'remember_me' %>Запомнить
					<%=true_button('Войти')%>
					<a>Забыли пароль?</a></div>
				<%- end -%>
				</div>
			<%else%>
				<h5>Привет!</h5>
				<div class="autorisation2">
				<%= current_user.login %>
				[<%= link_to "Выйти", {:controller=> 'account', :action => "logout"}%>]
				</div>
			<%end%>
			</div>
			
		</div>
		<div class="content_back">
			
			<div id="content_back_2" class="content_back_2"></div>
			<div id="box_content">
			<b class="r10"></b>
			<b class="r9"></b>
